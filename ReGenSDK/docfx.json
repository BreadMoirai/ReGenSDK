{
  "metadata": [
    {
      "src": [
        {
          "files": [ "../*.sln", "*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "."
        }
      ],
      "dest": "obj/api",
      "filter": "docfilter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.md", "*.md", "toc.yml", "" ]
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "ReGen SDK",
      "_enableSearch": true
    },
    "dest": "../docs",
    "template": [
      "default",
      "../docfx/mathew/src"
    ]
  }
}